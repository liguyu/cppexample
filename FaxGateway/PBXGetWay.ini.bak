#########################系统配置#######################################################
# XMSIP      系统流程IP                                                                #
# XMSPORT    系统流程端口                                                              #
# XMSUSER    系统用户                                                                  #
# XMSPASSWORD    用户密码                                                              #
# UNITID     应用程序ID 当系统有多个应用程序，需要配置不通ID                           #
# VOIPWORK   VOIP所在DSP ID 例如VOIPWORK=1，2，3，4 注意VOIPWORK和DIGTRKWORK必须相同   #
# DIGTRKWORK 数字中继所在DSPID 例如DIGTRKWORK=1,2,3,4                                  #     ########################################################################################
[XMSCONFIG]
XMSIP=192.168.11.131 
XMSPORT=9000         
XMSUSER=             
XMSPASSWORD=         
UNITID=20            
VOIPWORK=3           
DIGTRKWORK=3  
[XMSCONFIG]
XMSIP=192.168.11.131
XMSPORT=9000
XMSUSER=
XMSPASSWORD=
UNITID=20
VOIPWORK=1
DIGTRKWORK=1

###当通过中继呼出时,可以限制非法的主叫用户
[IPPOOL]
ITEMCNT=3
IP[1]=192.168.11.131
IP[2]=192.168.156.110
IP[3]=192.168.156.111

###把带内DTMF转换成带外DTMF
[RTPDTMF]
SENDTYPE=3
####// 1:H245UI 2:SIPINFO 3:RTPDTMF(2833)

###默认的proxy，当ROUTCNT=0时，使用默认proxy
[OUTBONDVOIP]
VOIP_ADDRESS=192.168.11.131
#######//被叫IP地址
VOIP_PORT=5077

###网关根据PSTN入网关的被叫号码前缀选择proxy，前缀不超过9位
[OUTBOUNDROUT]
ROUTCNT=2
ROUT[1]=192.168.11.131:5066,9
ROUT[2]=192.168.11.131:5066,8

###设置拨号规则
[DIALRULE]
##DIALRULE[1]=0591+2014731|.
DIALRULE[1]=0591+2014731|.
DIALRULE[2]=0592+2014731|.
DIALRULE[3]=0593+2014731|.
DIALRULE[4]=0594+2014731|.
DIALRULE[5]=0595+2014731|.
DIALRULE[6]=0596+2014731|.
DIALRULE[7]=0597+2014731|.
DIALRULE[8]=0598+2014731|.
DIALRULE[9]=0599+2014731|.
#### DIALRULE只支持xxxx+xxxx|.方式

###奇偶选路
[VOIPRULE]
OUTRULE = 1
#### OUTRULE =1 标识奇数通道用于呼出 = 0 标识偶数通道用于呼出

###写日志
[LOG] 
LOG = 1
#### LOG = 0 标识不写网关日志  = 1 写网关日志


###传真模式 1 T.38模式，0 T.30透传
[FAXMODE]
FaxMode=0

###设置传真信号音的模板
[GTD]
gtdid6=53
freqmask6=4
threshold6=0
envelop6=0
repeatcount6=1
minontime16=10
maxontime16=0
minofftime16=0
maxofftime16=0
minontime26=0
maxontime26=0
minofftime26=0
maxofftime26=0

gtdid7=54
freqmask7=8
threshold7=0
envelop7=0
repeatcount7=1
minontime17=10
maxontime17=0
minofftime17=0
maxofftime17=0
minontime27=0
maxontime27=0
minofftime27=0
maxofftime27=0




